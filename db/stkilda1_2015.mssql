(188, 17, '2012-festival-website', '2012 Festival website', '<p><span class="heading"><br />\r\n<span class="heading-content2">2013 ST KILDA FILM FESTIVAL</span><br />\r\n<br />\r\n</span><span class="heading">\r\n</span></p>\r\n<p><span class="heading-content3">Click<span style="color: #ffffff">&nbsp;</span><a target="_blank" href="http://www.stkildafilmfestival.com.au/2013-s2"><span style="color: #ffffff"><span style="color: #ffffff">HERE</span></span></a> to be magically transported back in time to the 2013 Festival website.</span></p>', '2013-03-19 14:49:39', '2014-03-26 04:41:55'),
(186, 17, 'tickets-info---stage-1', 'Tickets & Info - Stage 1', '<style type="text/css">@import url(http://www.stkildafilmfestival.com.au/2015/css/template.css);</style>\n<style type="text/css">@import url(http://www.stkildafilmfestival.com.au/2015/css/template.css);</style>\n<style type="text/css">@import url(http://www.stkildafilmfestival.com.au/2015/css/template.css);</style>\n<p class="normaltext1"><span class="heading"><img alt="" src="/2013/uploads/Tickets_710x300_2.jpg" /><br />\n<br />\n</span></p>\n<div class="calendar-purple-line"> </div>\n<span class="heading-content1">TICKETS &amp; INFO</span><span class="content-text"><span class="heading-content3"><span class=""><br />\n</span>Tickets available through</span><br />\n<br />\n<a target="_blank" href="http://www.ticketmaster.com.au/St-Kilda-Film-Festival-tickets/artist/1846524 "><img class="img-no-bootstrap" alt="" src="/Uploads/tmlogo_2011 gif.png" height="43" width="150" /></a></span><br />\n<br />\n<br />\n<span class="heading-content2"><a target="_blank" href="http://www.ticketmaster.com.au/St-Kilda-Film-Festival-tickets/artist/1846524 ">SINGLE TICKETS</a></span><br />\n<p>&nbsp;</p>\n<span class="content-text"><strong class="subheading2">\n<span class="subheading2">\n</span></strong><span class="heading-content3"><span class=""><span class="">SINGLE SESSION</span>:</span></span> $18 Full/$15 Concession <br />\n<span class="heading-content3"><span class="">SOUNDKILDA</span><span class="">:</span> </span>$27 Full/$22 Concession <br />\n<span class="heading-content3"><span class="">OPENING NIGHT</span><span class="">:</span> </span>$35 Full/$30 Concession<span class="subheading2"><br />\n</span><span class="heading-content3"><span class="">CLOSING NIGHT AWARDS</span><span class="">:</span> </span>$30 Full/$25 Concession<br />\n</span><span class="heading-content2"><br />\n<span class=""><a target="_blank" href="http://www.ticketmaster.com.au/event/13004C92AD698D7E ">FESTIVAL PASSES</a></span></span><br />\n<span class="heading-content3"><span class=""><br />\n<span class="">FILM BUFF’S FEAST:</span></span><span class=""> </span></span><span class="normaltext1"><span class="content-text">$100 Full/$90 Concession<br />\nThe complete degustation - your pass to all St Kilda Film Festival sessions. Sample all Top 100 Films and Festival screenings with the Film Buff’s Feast. <br />\n(Excluding Opening Night and Closing Night Awards).</span><br />\n<span class="heading-content3"><br />\n<span class="">5 SESSION PASS:</span><strong> </strong></span><span class="content-text">$70 Full/$60 Concession<br />\nTake your pick of five St Kilda Film Festival sessions and events. <br />\n(Excluding Opening Night, Closing Night Awards and SoundKILDA).<br />\n</span><br />\n</span><span class="heading-content3">ZOMEBIETHON GROUP BOOKINGS:</span> <span class="content-text">$13.50</span><span class="content-text"><span class=""> per person for groups of six or more</span></span><br />\n<span class="heading-content2"><span class=""><br />\nHOW TO BOOK</span></span><br />\n<span class="content-text"><span class="content-text">Tickets and Festival Passes for all screenings except Opening Night and Closing Night Awards will be available through TICKETMASTE</span>R.<br />\n<br />\n<span class="heading-content3">Online: </span><br />\n<a target="_blank" href="http://www.ticketmaster.com.au/St-Kilda-Film-Festival-tickets/artist/1846524 ">ticketmaster.com.au</a><br />\nMastercard, Visa, Amex and Diners<br />\n(Credit card fee applies)<br />\n$3.50 booking fee applies<br />\n<br />\n<span class="heading-content3">By phone: </span><br />\n13 61 00<br />\nMastercard, Visa, Amex and Diners<br />\n(Credit card fee applies)<br />\n$3.50 booking fee applies<br />\n<br />\n<span class="heading-content3">In person:</span> <br />\nPlease visit ticketmaster.com.au for Ticketmaster outlet locations. <br />\nTickets can also be purchased from the Box Office at The Astor Theatre during the Festival. <br />\n<br />\n<span class="heading-content2"><br />\nOPENING &amp; CLOSING NIGHT TICKETS</span><br />\n</span><span class="content-text"><span class="">Opening &amp; Closing Night tickets are strictly limited and will sell out. <br />\n<br />\nTo book phone 03 9209 6660</span>&nbsp;<span class="">or visit the St Kilda Film Festival office at Level 1, 232 Carlisle St, St Kilda East. (Office Hours 9am – 5pm weekdays). Cash, EFTPOS, Credit Card facilities available. </span>Opening and Closing Night tickets are sold on a no refund, exchange or cancellation basis. Lost or stolen tickets will not be replaced or refunded.</span><br />\n<span class="heading-content2"><span class=""><span class=""><br />\n<br />\nFINE PRINT</span></span></span><br />\n<span class="heading-content3"><br />\n<span class="">CONCESSION</span></span><br />\n<span class="content-text"><span class="">Concession applies to full time students, recipients of the Australian Social Security Pension, Victorian Seniors Card holders, aged pensioners and Triple R Subscriber Card holders. Concession ticket holders will be required to show proof of concession upon entry.</span></span><br />\n<br />\n<span class="heading-content3"><span class="">REFUND/EXCHANGES</span></span><br />\n<span class="content-text"><span class="">All tickets are sold on a no refund, exchange or cancellation basis. Lost or stolen tickets will not be replaced or refunded.</span></span><br />\n<br />\n<span class="heading-content3"><span class="">SEATING &amp; ACCESS</span></span><br />\n<span class="content-text"><span class="">All seating at St Kilda Film Festival screenings is general admission. Latecomers will not be admitted until a suitable break in the screening.</span><br />\n<br />\n<span class="">For information regarding disability access at venues, please contact Festival staff via the information hotline 03 9209 6711 or email filmfest@portphillip.vic.gov.au</span></span><br />\n<br />\n<span class="heading-content3"><span class="">CLASSIFICATION NOTICE</span></span><br />\n<span class="content-text"><span class="">As films featured in the St Kilda Film Festival are unclassified, persons under the age of 18 will not be admitted.</span></span><br />\n<br />\n<span class="normaltext1"><a target="_blank" href="ptv.vic.gov.au"><br />\n<br />\n<br />\n</a></span>', '2013-03-19 12:13:08', '2014-05-23 00:13:11'),
(159, 9, 'accommodation', 'ACCOMMODATION', '<p><span class="heading">ACCOMMODATION </span>\n<hr />\n</p>\n<p>&nbsp;</p>\n</font>\n<p>\n<table style="padding-top: 10px">\n    <tbody>\n        <tr>\n            <td class="heading"></td>\n            <td style="padding-top: 15px" valign="top" rowspan="5" width="250"><span class="normaltext1">&nbsp;</span></td>\n            <td style="padding-top: 15px" valign="top" rowspan="5" width="250"><br />\n            <span class="normaltext1"><img border="0" alt="" src="/Uploads/Accommodation_R.jpg" width="250" height="201" /></span></td>\n        </tr>\n        <tr>\n            <td style="width: 980px; height: 215px" class="normaltext1">\n            <p><span class="normaltext1"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><span class="subheading1"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><span class="subheading1"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><span class="subheading2"><font color="#702c91" face="Arial"><font class="subheading1" color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font class="subheading1" color="#702c91" face="Arial">COSMOPOLITAN HOTEL</font></font> </font></font></font></font><br />\n            <br />\n            </font></font></span></font></font></font></font></span></font></font></span><span style="font-size: 8pt"><span class="normaltext1"><span style="font-size: 10pt"><span class="normaltext1"><span class="normaltext1"><span style="font-size: 10pt"><span class="normaltext1"><span class="normaltext1"><span class="normaltext1">Located in the heart of St Kilda and on&nbsp;the doorstep of Luna Park, The Esplanade </span></span></span></span>and St Kilda Beach, the Cosmopolitan Hotel stands out with its comfortable interiors, fantastic service and incomparable value for money. This landmark icon has been im<span style="font-size: 10pt">maculat</span>ely redesigned with the latest luxury amenities and variety of accommodation types<span class="normaltext1">, from single rooms to two bedroom apartments. The Cosmopolitan Hotel is perfectly positioned to insure an unforgettable and truly Ã¢â‚¬ËœMelbourneÃ¢â‚¬â„¢ experience</span></span></span><span class="normaltext1">.</span><br />\n            &nbsp;<br />\n            <span class="normaltext1">2-8 Carlisle Street, St Kilda VIC 3182<br />\n            (03) 9534 0781 <br />\n            <a href="http://www.8hotels.com/melbourne-hotels/cosmopolitan-hotel/" target="_blank"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1">cosmopolitanhotel.com.au</span></font></span></a></span></span></span></span></span></font></span></font></span></span></p>\n            <p><span class="normaltext1"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><font color="#702c91" face="Arial"></font></span></font></span></span>&nbsp;</p>\n            <p><span class="normaltext1"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><img style="width: 130px; height: 85px" border="0" alt="" src="/Uploads/Cosmo_R.jpg" width="130" height="85" /></span></font></span></font></span></span></p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n        <tr>\n            <td>\n            <p><span class="normaltext1"><em></em></span>&nbsp;</p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n    </tbody>\n</table>\n</p>\n', '2012-05-01 10:13:07', '2012-05-15 17:47:26'),
(251, 17, 'captioned-screening', 'Captioned Screening', '<img src="/2015/uploads/Header-CaptionedScreening.jpg" alt="" height="345" width="850" />\n<div style="height: 20px">&nbsp;</div>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table">\n    <tbody>\n        <tr>\n            <td><span class="heading-content1">CAPTIONED</span> <span class="heading-content1">SCREENING</span><br />\n            <span class="heading-content2">OPENING NIGHT ENCORE</span><br />\n            <span class="heading-content3"><span class="">SATURDAY 31 MAY, 1.15PM<br />\n            THE ASTOR THEATRE</span></span><br />\n            <br />\n            <a target="_blank" href="http://www.ticketmaster.com.au/event/13004C92E34FD452"><img class="img-no-bootstrap" alt="" src="http://www.stkildafilmfestival.com.au/2015/uploads/BUY-TICKETS-BIG.png" height="44" width="150" /></a><br />\n            <br />\n            <span class="content-text">This year, the St Kilda Film Festival will be presenting an encore screening of the Opening Night film selection, with captions. As films featured in the St Kilda Film Festival are unclassified, persons under the age of 18 will not be admitted.<br />\n            <br />\n            </span>\n            <p><span class="content-text">Proudly supported by Red Bee Media </span></p>\n            <p>&nbsp;</p>\n            <p>&nbsp;<img class="img-no-bootstrap" alt="" src="http://www.stkildafilmfestival.com.au/2015/uploads/RedBee.png" height="49" width="50" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <img class="img-no-bootstrap" alt="" src="http://www.stkildafilmfestival.com.au/2015/uploads/OC-logo.png" height="50" width="50" /></p>\n            </td>\n        </tr>\n    </tbody>\n</table>\n<br />\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">Almost</span><br />\n            <span class="heading-content3">World premiere</span><br />\n            <span class="content-text">12.32mins, NSW, 2012<br />\n            <br />\n            Emily and Tom meet and have a magnetic connection but fate has a different plan for their new love.<br />\n            <br />\n            Director Frances Anderson<br />\n            Producer Belinda Dean, Sonia Todd</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/ON-Almost.jpg" alt="" width="0" /> </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">‘Twin Rivers’ Big Scary</span><br />\n            <span class="heading-content3">SOUNDKILDA</span><br />\n            <span class="content-text">4.03mins, VIC, 2013<br />\n            <br />\n            A praying mantis struggles to find contentment in a modern world.<br />\n            <br />\n            Director Shaun Garland<br />\n            Producer Maxine Chaplin</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/TwinRivers-BigScary-2.jpg" alt="" width="0" /> </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">The Video Dating Tape of Desmondo Ray, Aged 33 &amp; 3/4</span><br />\n            <span class="content-text">3.20mins, QLD, 2013<br />\n            <br />\n            |Desmondo Ray enjoys peeing in the rain, altering offensive graffiti, and listening to sad music while having happy thoughts. This is his video dating tape.<br />\n            <br />\n            Director Steve Baker<br />\n            Producer Steve Baker, Laura Mustchin</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/ON-TheVideoDatingTapeOfDesmondoRay.jpg" alt="" width="0" /> </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">Test Drive</span><br />\n            <span class="heading-content3">World premiere</span><br />\n            <span class="content-text">11.02mins, NSW, 2013<br />\n            <br />\n            A test drive turns sinister when Sadie gets trapped in the backseat of her own car, captive to brothers whose relationship is set to explode. <br />\n            <br />\n            Director Lynne Vincent Mccarthy<br />\n            Producer Samantha Jennings, Angus Stevens</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/ON-TestDrive.jpg" alt="" width="0" /> </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">Life on Tape</span><br />\n            <span class="content-text">7.35mins, VIC, 2013<br />\n            <br />\n            Life on Tape follows the obsession of John Flaus, a well-known Australian actor, voice talent and film academic, as he tries to document the world in its current state. <br />\n            <br />\n            Director Oscar Strangio<br />\n            Producer David Valkenet</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/ON-LifeOnTape.jpg" alt="" width="0" /> </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">A Writer and Three Script Editors Walk Into a Bar</span><br />\n            <span class="heading-content3">World premiere</span><br />\n            <span class="content-text">2.30mins, VIC, 2013<br />\n            <br />\n            A writer tells a joke. Complications ensue.<br />\n            <br />\n            Director Matthew Saville<br />\n            Producer Matthew Saville</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/ON-AWriter3ScriptEditors.jpg" alt="" width="0" /> </td>\n        </tr>\n        <tr>\n            <td valign="top"><br />\n            </td>\n            <td valign="top"><br />\n            </td>\n            <td valign="top"><br />\n            </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">Possum</span><br />\n            <span class="heading-content3">Victorian premiere</span><br />\n            <span class="content-text">16.14mins, VIC/SA, 2013<br />\n            <br />\n            On the weekend of Halloween, an unsettling encounter with a dead possum forces a grieving young couple to reconnect.<br />\n            <br />\n            Director Eleanor Wilson<br />\n            Producer Eleanor Wilson, Rosebud Baker</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/ON-Possum.jpg" alt="" width="0" /> </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">‘Hometown’ Munro Melano</span><br />\n            <span class="heading-content3">SOUNDKILDA</span><br />\n            <span class="content-text">4.01mins, VIC, 2013<br />\n            <br />\n            A love letter to Australian musicians on the road, past and present. In 1959, 17-year-old Leon Isackson played the drums in a Sydney rock ''n'' roll band, recording 8mm road movies of life on tour. This music video is composed entirely of those films.<br />\n            <br />\n            Director Sari Braithwaite<br />\n            Producer Sari Braithwaite</span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/HOMETOWN-MunroMelano.jpg" alt="" width="0" /> </td>\n        </tr>\n    </tbody>\n</table>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table calendar-with-bottom-gap">\n    <tbody>\n        <tr>\n            <td style="width: 70%;"><span class="heading-content2">Chopper</span><br />\n            <span class="heading-content3">Palm Springs International ShortFest</span><br />\n            <span class="content-text">2mins, Netherlands, 2012 <br />\n            <br />\n            </span><span class="content-text">A highly entertaining, rapid-fire run down of how the food chain works. Have you had your Wheaties today?<br />\n            <span class=""><br />\n            Director Lars Damoiseaux, Frederik Palmaers<br />\n            Producer Sander Verdonk, Denis Wigman<br />\n            </span></span><br />\n            </td>\n            <td style="width: 5%;"> <br />\n            </td>\n            <td style="width: 25%;"> <img src="/2015/uploads/ON-Chopper.jpg" alt="" width="0" /> </td>\n        </tr>\n        <tr>\n            <td valign="top"><br />\n            </td>\n            <td valign="top"><br />\n            </td>\n            <td valign="top"><br />\n            </td>\n        </tr>\n    </tbody>\n</table>', '2014-04-27 23:16:09', '2014-04-29 01:43:45'),
(225, 17, 'about-touring---2014', 'About Touring - 2014', '<img src="/2015/uploads/Touring.jpg" alt="" />\n<div style="height: 20px">&nbsp;</div>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table">\n    <tbody>\n        <tr>\n            <td>\n            <h1 class="heading-content1">SHORT FILMS ON TOUR<br />\n            </h1>\n            <h2 class="heading-content2">2014 ST KILDA FILM FESTIVAL TOUR KICKS OF IN JUNE<br />\n            </h2>\n            <h3 class="heading-content3">Full program details available mid-May<br />\n            </h3>\n            <p class="content-text"> 				<br />\n            When the Closing Night party draws to a close in Melbourne, the St Kilda Film Festival Tour prepares to kick off.<br />\n            <br />\n            Travelling through a host of regional destinations, the Tour presents the highlights of the Festival at special screening events across the country.<br />\n            <br />\n            Festival Director Paul Harris says, “The awesome shorts chosen for our Tour showcase offer audiences the opportunity to view some of the most innovative, cutting edge, inspiring and just plain entertaining work from Australia’s current crop of emerging filmmakers, who are literally taking the world by storm.”<br />\n            <br />\n            The St Kilda Film Festival recognises that people outside of capital cities do not often have access to short films, particularly those of such high quality as those screened on the Tour. The Tour takes these films to destinations far and wide, and encourages filmmakers in these areas to enter their films into the Festival in future years.<br />\n            <br />\n            It’s about giving a broad audience the opportunity to watch, discuss and create short films, maximising screening opportunities for the top films and filmmakers from the Festival and helping audiences in remote locations to access diverse screen culture.<br />\n            <br />\n            Interested in hosting the St Kilda Film Festival Tour in your home town? Contact us at <a target="_blank" href="&#109;&#97;&#105;&#108;&#116;&#111;&#58;&#102;&#105;&#108;&#109;&#102;&#101;&#115;&#116;&#64;&#112;&#111;&#114;&#116;&#112;&#104;&#105;&#108;&#108;&#105;&#112;&#46;&#118;&#105;&#99;&#46;&#103;&#111;&#118;&#46;&#97;&#117;">filmfest@portphillip.vic.gov.au</a> to find out more.<br />\n            <br />\n            </p>\n            </td>\n        </tr>\n    </tbody>\n</table>', '2014-03-26 20:59:27', '2014-04-14 05:56:33'),
(197, 1, 'airasia-festival-lounge', 'AirAsia Festival Lounge', '<img src="http://www.stkildafilmfestival.com.au/Uploads/AirAsiaLounge-FullWidth-3.jpg" alt="" width="850" height="359" />\n<div style="height: 20px">&nbsp;</div>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table">\n    <tbody>\n        <tr>\n            <td>\n            <h1 class="heading-content1">THE <img class="img-no-bootstrap" alt="" src="http://www.stkildafilmfestival.com.au/Uploads/AirAsiaLogo.png" width="95" align="textTop" height="95" /> LOUNGE</h1>\n            <p class="content-text"> <br />\n            <span class="content-text">Proudly brought to you by AirAsia, the official Festival lounge is upstairs at The Astor Theatre. It’s the perfect place to relax, talk film with friends, network and soak up the atmosphere. <br />\n            <br />\n            Have a coffee, buy a choc top or while away the night with a glass of wine or beer.<br />\n            <br />\n            For ten days at the end of May, it’s the best club in town. <br />\n            </span><br />\n            </p>\n            </td>\n        </tr>\n    </tbody>\n</table>', '2013-04-15 15:25:13', '2014-04-16 01:49:21'),
(199, 17, 'accommodation', 'Accommodation', '<span class="heading"><img src="/2015/uploads/HotelUrban.jpg" alt="" height="400" width="850" /></span>\n<div style="height: 20px">&nbsp;</div>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table">\n    <tbody>\n        <tr>\n            <td>\n            <span class="heading-content1">HOTEL URBAN</span>\n            <br />\n            <span class="heading-content3">2014 St Kilda Festival Accommodation Partner\n            <br />\n            </span><span class="content-text"><span class=""><br />\n            </span>This is not your typical hotel accommodation. Hotel Urban St Kilda is more than a hotel; offering more comfort, more indulgence, more choice and more inspiration.\n            Only 200 metres from the beach, the boutique Hotel Urban St Kilda will suit those who value designer chic combined with relaxed, modern comfort. Complete with conference facilities and chic Hub Cafe and Mavis Brown''s Bar, this uniquely stylish pad combines some unique hotel features with plenty of guest room perks. With 4 ultra-comfy and unique room types, premium amenities and our signature Urban Snorer beds (specially designed for the ultimate slumber), it’s the next best thing to sleeping at home.\n            <br />\n            <br />\n            Come on over and experience the Urban difference for yourself!\n            <br />\n            <br />\n            <a target="_blank" href="http://hotelurban.com.au/melbourne ">hotelurban.com.au/melbourne\n            </a>\n            <br />\n            </span><span class="heading-content3"><span class="">35-37 Fitzroy Street, St Kilda, VIC 3182\n            <br />\n            Tel: +61 3 8530 8888\n            </span></span><br />\n            <p>&nbsp;</p>\n            </td>\n        </tr>\n    </tbody>\n</table>', '2013-04-15 15:39:28', '2014-04-22 22:06:16');
INSERT INTO `pages` (`id`, `category_id`, `name`, `title`, `content`, `created`, `modified`) VALUES
(194, 15, '2013--the-big-picture', '2013 -The Big Picture', '<p class="normaltext1"><span class="heading"><br />\nTHE BIG PICTURE</span><span><span class="subheading1"><br />\n</span></span><br />\nPresented by Deluxe Entertainment Services Group<br />\n<br />\n<a target="_blank" href="http://www.bydeluxe.com"><img alt="" src="/Uploads/DeluxeRedCircle.png" width="95" height="95" /></a><br />\n</p>\n<p><span class="normaltext2"><span class=""><span class="subheading2">\n<br />\n</span></span></span><span class="subheading1">THE ASTOR THEATRE<br />\n</span><span class="subheading2"><span class=""><span class="">SATURDAY 25 MAY<br />\n11AM-4PM</span></span></span><br />\n</p>\n<p><span class="normaltext2"><span class=""><span class=""><br />\nThe film industry takes over The Astor for one big day of professional development.<br />\n<br />\nAs part of the St Kilda Film Festival’s Filmmaker Development program, The Big Picture covers all you need to know about making a short film, from start to finish. With forum topics including scriptwriting, funding, legal, production and distribution, you will end the day with a wealth of knowledge from some of the industry’s most experienced and awarded filmmakers and film professionals. Moderated by Shaun Miller.<br />\n<br />\nAnd if that isn’t enough, you can continue the conversations face-to-face with stallholders at our <strong>Industry Open Day</strong>, with representatives from pre- and post-production houses, education bodies and filmmaker guilds hosting information booths upstairs at The Astor.</span></span></span><br />\n<span class="normaltext1"><br />\n</span><span class="normaltext1">Following The Big Picture, join us for an ongoing series of talks happening <a href="http://www.stkildafilmfestival.com.au/2013-s2/page-195/2013---throughout-the-week">throughout the Festival week</a>. </span><span class="normaltext1">All Filmmaker Development events are FREE. Entry is on a first come, first served basis.</span><br />\n</p>\n<hr />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <span class="subheading1"><span class="">THE WRITE STUFF</span></span><br />\n            <span class="subheading2">11.10AM</span><br />\n            <br />\n            The job of a screenwriter is to tell a story and to tell it well. But how do you do that? What are the essential ingredients and how do you know if writing is the right path for you? Join writers Peter Moon, Mac Gudgeon (pictured), Kris Mrksa and Natasha Pincus as they explore the ups and downs of the writing process and the collaborations that have helped make their projects successful.<font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"><span class="normaltext2"><br />\n            <br />\n            Presented by the Australian Writers Guild&nbsp; <a target="_blank" href="http://www.awg.com.au"><img alt="" src="/Uploads/awg-5years-logo-HERO-cmyk.jpg" width="75" align="top" height="44" /></a></span></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-TheWriteStuff-MacGudgeon.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">SHOW ME THE MONEY</font><font class="subheading2" face="Lato"><br />\n            12.05PM</font><br />\n            <br />\n            Money doesn’t grow on trees, and neither does funding for short film (unfortunately!). Finance for short films can be sourced from private investors and donors, government funding, crowd funding, deferrals, “sweat equity” or in-kind favours. This session will look at the “how, why, where and when” each alternative works best. You will then be in a better informed position to pay for the realisation of your short film. Panellists: Film Victoria''s Acting Head of Screen Industry Programs Sue Edwards, Screen Australia''s Development Executive Katherine Slattery, producers Jannine Barnes and Michael Ciccone, and Anna Whitelaw, crowd funding for film expert. Photo by James Thomas.<br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-ShowMetheMoney-Ciccone.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">LAW, CAMERA, ACTION!</font><font class="subheading2" face="Lato"><br />\n            1PM<br />\n            <br />\n            </font> </p>\n            <p> This session will explore legal issues relating to financing, production and distribution of films and television which every emerging filmmaker should be aware of. The session will be interactive with the audience where you get to cross-examine Jenny Lalor and Shaun Miller (pictured) while they provide the evidence to hopefully let you rest your case. Come along and witness this legal drama!<br />\n            <br />\n            Presented by Shaun Miller Lawyers <a target="_blank" href="http://www.shaunmillerlawyers.com.au"><img alt="" src="/Uploads/ShaunMiller-Logo.jpg" width="186" align="top" height="27" /></a><br />\n            <font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-LawCameraAction-ShaunMiller.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">PRODUCTION: THE CHANGING TECHNOLOGICAL LANDSCAPE</font><font class="subheading2" face="Lato"><br />\n            1.50PM</font> </p>\n            <p> <br />\n            The creators of award winning series <em>The Slap</em> are stopping by to talk production – pre-production, the shoot and post processes. Digital production is changing the way we make films – not just in the way we store files and the choice in digital cameras, but also the way we shoot and the way we grade and finish. Come along to this forum to learn from the people that get it right. Panellists: Matt Saville, Michael McMahon, Ross Mitchell, Tim Schumann and Melinda Doring.</p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-Production-TheSlap.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">DISTRIBUTING FILMS: THE MORE PLATFORMS THE MERRIER</span><br />\n            <span class="subheading2">3PM<br />\n            </span><br />\n            </p>\n            <p class="normaltext1"> Get the lowdown on how best to distribute and market your short film both locally and internationally. This session will explore the array of media platforms in which short films can be distributed and marketed including film festivals, television and new media platforms such as mobile phones and other portable devices. Commercial tips will be offered and legal pitfalls to be wary of will be highlighted. Panellists: Grant Scicluna (pictured<em>, The Wilding</em>), Simon Britton (MediaWave TV), Katharine Thornton (Distribution Manager Sharmill Films), Claudette Godfrey (Short Film Programmer &amp; Film Festival Coordinator at SXSW) and Kristian Connelly (Cinema Nova General Manager).</p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-Distribution-GrantScicluna.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">LIVE BROADCAST: FILMBUFF''S FORECAST</span><br />\n            <span class="subheading2">12PM-2PM<br />\n            </span><br />\n            </p>\n            <p class="normaltext1"> Don’t miss Festival Director <a target="_self" href="http://www.stkildafilmfestival.com.au/2013-s2/page-179/festival-director">Paul Harris</a> in Filmbuff’s Forecast, broadcast live from the Astor Foyer Theatre from 12pm until 2pm.</p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/RRR-Filmbuffs.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />', '2013-04-12 14:30:54', '2013-05-14 11:13:35'),
(195, 15, '2013---throughout-the-week', '2013 - Throughout the Week', '<p><span class="heading"><br />\nTHROUGHOUT THE WEEK<br />\n</span><br />\n</p>\n<p class="normaltext1"><span class="normaltext1">Enjoy a day of forums on Saturday 25 May with <a href="http://www.stkildafilmfestival.com.au/2013-s2/page-194/2013--the-big-picture">The Big Picture</a> then join us for an ongoing series of talks happening throughout the Festival week.<br />\n<br />\nAll of the below events will be held at the Forum Hall in All Saints Church, opposite The Astor on the corner of Chapel St &amp; Dandenong Rd, St Kilda</span><br />\n<br />\nAll Filmmaker Development events are FREE. Entry is on a first come, first served basis. <br />\n</p>\n<hr />\n<a name="perfectmatch"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p><span class="subheading1">FINDING YOUR PERFECT MATCH: RELATIONSHIP BETWEEN THE DIRECTOR AND CINEMATOGRAPHER</span><br />\n            <font class="subheading2" face="Lato">MONDAY 27 MAY, 6.30PM</font><br />\n            <br />\n            The relationship between the Director and the Cinematographer has often been likened to a creative ''marriage''. Like any successful relationship it takes commitment, respect, trust and understanding. This forum will explore some of the traits essential for successful creative collaborations, as well as techniques to get the best out of each other. Featuring collaborators Natasha Pincus and Warwick Field. Moderated by Victoria Goodyear.<br />\n            <br />\n            </p>\n            <p>\n            Presented by Australian Directors Guild and Australian Cinematographers Society<br />\n            <br />\n            </p>\n            <p><a target="_blank" href="http://www.adg.org.au/"><img alt="" src="/Uploads/ADG-logo.png" width="60" height="52" /></a>&nbsp;&nbsp;&nbsp; <a target="_blank" href="http://www.cinematographer.org.au"><img alt="" src="/Uploads/acs-logo.png" width="60" height="41" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-CineDir.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="spaa"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <span class="subheading1">SPAA FRINGE WORKSHOP - PITCHING TO A TV CHANNEL</span><br />\n            <font class="subheading2" face="Lato">MONDAY 27 MAY, 8PM</font><br />\n            <br />\n            The opportunity to pitch your idea direct to a TV channel is rare. Now here’s your chance! <br />\n            <br />\n            SPAA has teamed up with STUDIO, FOXTEL’s arts and entertainment channel to offer early-career producers the chance to get a project commissioned for the channel through SPAA''s new Ones to Watch mentor program and the STUDIO Kickstart initiative. Finalists can pitch their project and go into the running to receive a production budget of $300K. This workshop provides insight into applying for SPAA''s Ones to Watch program and STUDIOs exciting new production initiative.<br />\n            <br />\n            Presented by SPAA<br />\n            <a target="_blank" href="http://www.spaa.org.au"><img alt="" src="/Uploads/SPAA_logo.png" width="125" height="62" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-SPAA.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="thisisthespot"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">THIS IS THE SPOT</font><br />\n            <font class="subheading2" face="Lato">TUESDAY 28 MAY, 6.30PM</font><br />\n            <br />\n            What makes a good Location Manager? Finding a creative location that works well for the Director and Production Designer is one thing - you also need to navigate the red tape, keep within budget and schedule. The role of a Location Manager is vital and knowing the tricks of the trade can make or break a shoot. Get advice from the experts who''ve successfully sourced and managed locations for <em>The Slap, Rush, Tangle, Mr and Mrs Murder, Canal Road, Miss Fisher''s Murder Mysteries, Ghost Rider</em> and <em>The Pacific</em>. Moderated by Neil McCart with panellists Pia Emery, Mel Dunstone, Alistair Riley and Drew Rhodes.<br />\n            <br />\n            <br />\n            <br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="http://www.stkildafilmfestival.com.au/Uploads/Top%20100_R.jpg" width="230" border="0" height="150" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="ifiknewthen"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">IF I KNEW THEN WHAT I KNOW NOW</font><br />\n            <font class="subheading2" face="Lato">TUESDAY 28 MAY, 8PM</font> </p>\n            <p> <br />\n            Can you learn from other people''s hits and misses? Is it true that what doesn''t kill you makes you stronger? This lively panel may not exactly answer those questions, but it will have some fun exploring the journeys of some of the Festival''s favourite filmmakers along the way. Panellists: Clayton Jacobson, Emma Freeman, Julie Eckersley and Trevor Blainey. Moderated by Adam Elliot (pictured).<br />\n            <br />\n            <br />\n            <br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="http://www.stkildafilmfestival.com.au/Uploads/FD-AdamElliot.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="focusonediting"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">FOCUS ON EDITING</span><br />\n            <span class="subheading2">WEDNESDAY 29 MAY, 6.30PM <br />\n            <br />\n            </span></p>\n            <p> <span class="normaltext1">Australia’s peak representational body for screen editing brings together some of the country’s finest editing talent to share their own techniques, knowledge and experiences. The session will use examples of each panellist’s work to dissect and analyse the post-production process. Open to directors, producers, editors and those captivated by the intricacies of this craft. <br />\n            <br />\n            Presented by Australian Screen Editors Guild<br />\n            <br />\n            <a target="_blank" href="http://www.screeneditors.com"><img alt="" src="/Uploads/ASE_logo.png" width="150" align="textTop" height="42" /></a></span></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-Editing.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="wannabe"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1"><span class="">SO, YOU WANNA BE A FILMMAKER?</span></span><br />\n            <span class="subheading2">WEDNESDAY 29 MAY, 8PM <br />\n            <br />\n            </span></p>\n            <p> <span class="normaltext1">An insight into the realities of working in the film industry, the highs and lows and the real opportunities that exist for emerging and aspiring filmmakers. Moderated by Caroline Waters, a panel of working practitioners including Jonathan auf der Heide, Darrell Martin, Gretchen Thornburn, Julie Belle and Daniel Schultheis will share their experiences and advice in this practical session.<br />\n            <br />\n            Presented by Open Channel </span><br />\n            <br />\n            <br />\n            <a target="_blank" href="http://www.openchannel.org.au"><img alt="" src="/Uploads/OpenChannelLogo.png" width="125" height="42" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-wannabeafilm.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="composing"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1"><span class="">THE ABCS OF COMPOSING FOR SCREEN</span></span><br />\n            <span class="subheading2">THURSDAY 30 MAY</span><span class="subheading2">, 6.30PM</span><br />\n            <span class="normaltext1"><br />\n            A special forum on composing original underscore for film and TV, with the simple aim of equipping filmmakers and composers with knowledge, language and insights to obtain the best results for their films. Some of Australia’s leading composers will join forces to share how music relates to the story and enhances the screen experience. Panellists include Cezary Skubiszewski (<em>The Sapphires, Red Dog, Bran Nue Dae</em>), Burkhard Dallwitz (pictured, <em>Underbelly</em>), Thomas Rouch (<em>Reverse Runner</em>) and Petra Salsjo (<em>Dave’s Dead</em>).<br />\n            <br />\n            Presented by APRA</span><br />\n            <br />\n            <a target="_blank" href="http://www.apra-amcos.com.au"><img alt="" src="/Uploads/APRA-REV.jpg" width="87" height="91" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-Composing.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="reclaiming"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">RECLAIMING IDENTITY: <br />\n            </span><span class="subheading1"><span class="">THE CHANGING REPRESENTATION OF ABORIGINAL AND TORRES STRAIT ISLANDER PEOPLES IN FILM</span></span><br />\n            <span class="subheading2">FRIDAY 31 MAY, 6.30PM</span><br />\n            <br />\n            <span class="normaltext1"> Join the National Indigenous Television Network, legend </span><span class="normaltext1"><span class="">Uncle Noel Tovey, producer/director John Harvey, feminist blogger Celeste Liddle</span><span class=""> </span><span class="">and Margaret Harvey</span><span class=""> in a dynamic discussion about how Aboriginal and Torres Strait Islander peoples are claiming their place in the Australian film industry. </span><span class="">Moderated by Todd Condie, Indigenous Policy Officer, City of Port Phillip, </span><span class="">this forum will be held on the land of the Yalukit Wilum clan of the Boon Wurrung language group of the greater Kulin Nation.</span></span><br />\n            <br />\n            </p>\n            <p><span class="normaltext1">John Harvey is an emerging writer, director and producer in film and theatre with his company Brown Cab Productions and is currently working with highly acclaimed filmmaker Robert Connolly as a Producer Intern. Enduring a poverty-stricken and turbulent start to life, Noel Tovey (pictured) has led an esteemed career spanning 60 years as an actor, director, dancer, singer, choreographer, designer, w</span><span class="normaltext1"><span class="">riter and teacher – a career that has spanned Europe and Australia. Celeste Liddle is an Arrernte woman, National Indigenous Organiser of the NTEU and feminist blogger. She completed her BA Honours in theatre and drama in 2002 looking at identity politics in Indigenous Australian theatre.</span> Margaret Harvey is an emerging stage and film director based in Melbourne.  Her passion lies in exploring cross art forms to tell a story. <br />\n            <br />\n            </span></p>\n            <p class="normaltext1">Presented with the assistance of the National Film and Sound Archive of Australia <br />\n            </p>\n            <p><br />\n            <img alt="" src="/Uploads/NFSA-Mono-REV.png" width="153" height="88" /><br />\n            <br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FD-NoelTovey.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />', '2013-04-12 15:17:22', '2013-05-28 15:30:04'),
(200, 7, 'easy-come-easy-go', 'Easy Come Easy Go', '<p><span class="heading"><img alt="" src="/Uploads/Easybeats-ResizedforWeb-2.jpg" width="710" height="300" /></span></p>\n<span class="heading"><br />\nEASY COME, EASY GO\n</span><br />\n<span class="subheading1">A SALUTE TO AUSSIE POP IN THE SIXTIES</span><br />\n<span class="subheading2"><br />\nMONDAY 27 MAY, 7.30PM<br />\nTHE ASTOR THEATRE</span><br />\n<br />\n<a target="_blank" href="http://www.ticketmaster.com.au/event/13004A8FE0C2A89F"><img alt="" src="/Uploads/BUY-TICKETS-BUTTON.jpg" width="124" height="30" /></a><br />\n<p><span class="normaltext1"><br />\n<br />\nA night of quintessential sixties Aussie pop starring The Easybeats in the long lost documentary <em>Easy Come, Easy Go</em>, not screened in Melbourne in over 40 years</span><span class="normaltext1">.<br />\n<br />\nThis session proudly supported by the National Film and Sound Archive of Australia</span><br />\n<br />\n<br />\n<a target="_blank" href="http://www.nfsa.gov.au/"><img alt="" src="/Uploads/NFSA-Mono-REV.png" width="165" height="96" /></a><br />\n</p>\n<span class="normaltext1">\n</span>\n<hr />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <span class="subheading1">EASY COME, EASY GO</span><br />\n            <span class="normaltext1">(Director: Peter Clifton, 1967/2011, 33.08mins)<br />\n            <br />\n            Director Peter Clifton shot his Easybeats documentary in 1967, but the negative and the print vanished during post-production and the project was forgotten…until recently. Clifton has restored the film to a 35-minute version which will have its Victorian premiere screening at the St Kilda Film Festival.<br />\n            <br />\n            See the band perform their international hit ‘Friday On My Mind’, flee teenyboppers down Carnaby Street, visit the pirate broadcaster Radio Caroline and enjoy a footy match with the Small Faces and much more! If you’re a fan of Vanda and Young or Little Stevie Wright, this is surely must-see viewing, with a soundtrack of hits and rarities pounding out through the Astor sound system.<br />\n            <br />\n            The new version has been restored with the assistance of the National Film and Sound Archive. Director Peter Clifton will introduce the screening.</span><br />\n            <p><font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/Easybeats-Small-Right.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <span class="subheading1">MELDRUM 1971</span><br />\n            <span class="normaltext1">(Director: Peter Lamb, 1971, 8mins)<br />\n            </span><span class="normaltext1"><span class=""><br />\n            </span>In November 1974 Australia''s most popular music show, Countdown, premiered on ABC television. A few months later pop guru Ian " Molly " Meldrum became the face of the show with his shambolic interviews, enthusiastic endorsements of numerous bands and general gossip. Molly was already an industry veteran with his ties to Go-Set magazine as a pop columnist and as a producer, most notably on Russell Morris'' ‘The Real Thing’ (1969) plus hits for Ronnie Burns and Zoot.<br />\n            <br />\n            But Countdown was not Molly''s first stab at television. In the late 1960''s he was a regular on Kommotion and Uptight, but he had yet to host his own show. In 1971 an opportunity presented itself when he starred in an eight-minute pilot in which he pitched '' a new concept in television '' - a music-pop culture show aimed squarely at teen and young adult tastes. The pitch, directed by Garry Wapshott and Peter Lamb, is a fascinating time capsule. Molly is seen in full sartorial splendour (minus his later trademark hat) speaking direct to camera, passionately and (would you believe) most eloquently. This footage has never been screened in public.<br />\n            <br />\n            "We reject false slickness and intend to present our program with honesty and integrity, creative value and commercial appeal,” Molly Meldrum enthuses.<br />\n            <br />\n            The pitch fell on deaf ears at the time, but three years later a legend was born.<br />\n            <br />\n            Meldrum is a late addition to the Easy Come Easy Go program. Also added to the line-up are rare clips from Axiom (‘Arkansas Grass’) and The Twilights (‘Cathy Come Home’, ‘The Way They Played’).</span><br />\n            <p><font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/Meldrum-SmallRightPic.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;"><span class="subheading1">APPROXIMATELY PANTHER</span><br />\n            <span class="normaltext1">(Dir: Peter Lamb, 1966, 22.26mins)<br />\n            <br />\n            <em>Approximately Panther</em> is a unique document of the burgeoning Melbourne music scene as it existed nearly half a century ago. Shot by Peter Lamb and Garry Wapshott, the film features rare footage of bands and venues; a fascinating time capsule into a vanished era of once-groovy fashion and trendy discotheques.<br />\n            <br />\n            Follow Doug Panther, ace hipster and go-set journalist through Swingin'' Melbourne circa 1967. The cast of characters includes singer Lynne Randell, model Jennie Ham, the skivvie-clad 3AK Good Guy disc jockey Lionel Yorke and pop sensation Normie Rowe, mobbed by adoring fans at Essendon Airport. We also catch a glimpse of Melbourne''s favourite cult group The Loved Ones.</span></td>\n            <td style="padding-top: 15px;" valign="top" width="10"><br />\n            </td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/Approximately-Sm-R.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top"><span class="subheading1">FUN RADIO</span><br />\n            <span class="normaltext1">(Dir: Nigel Buesst, 1964, 9.22mins)<br />\n            <br />\n            Don''t let anyone tell you that Melbourne didn''t rock in the sixties. The epicentre was The Greater 3UZ (930AM), the radio station with its finger on the pulse of teen tastes. Director Nigel Buesst, a founding father of St Kilda Film Festival, focuses his springwind Bolex camera on Professor Fun (Don Lunn), the city''s most popular disc jockey of the time, presiding over a kaleidoscope of jingles, news flashes, promos, hit singles and lame jokes.</span></td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/Uploads/FunRadio-Sm-R.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p class="subheading1">ALSO SCREENING</p>\n            <p><span class="subheading2"><br />\n            SAD DARK EYES</span> <br />\n            </p>\n            <span class="normaltext1">(Dir: Peter Lamb, 1967, 2.14mins)<br />\n            <br />\n            A rarely seen music video by The Loved Ones. &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp;</span> <br />\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="http://www.stkildafilmfestival.com.au/Uploads/TheLovedOnes-SmallRight.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />', '2013-04-15 15:54:25', '2013-05-14 17:04:16'),
